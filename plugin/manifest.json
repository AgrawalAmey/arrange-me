{
  "name": "Arrange-Me",
  "version": "1.1",
  "description": "A chrome plugin to automatically cluster bookmarks and place them in corresponding folders.",
  "permissions": [
    "bookmarks",
    "http://*/*",
		"https://*/*"
  ],
  "browser_action": {
      "default_title": "Arrange-Me",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
